<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="login_et">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
        <item name="android:background">@null</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:layout_marginBottom">@dimen/dp_5</item>
        <item name="android:textCursorDrawable">@drawable/et_cursor</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:gravity">bottom</item>
    </style>

    <style name="login_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/main_text_color</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:layout_marginTop">@dimen/dp_20</item>
    </style>

    <style name="login_et_blow_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_0_5</item>
        <item name="android:background">@color/gray_shallow</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>

    <style name="radio_group_item">
        <item name="android:layout_width">@dimen/dp_0</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/white</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">@dimen/dp_3</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/index_text_color_selecter</item>
        <item name="android:textSize">@dimen/sp_16</item>
    </style>
    <style name="radio_group_item_add_goods">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_80</item>
        <item name="android:background">@color/white</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">@dimen/dp_3</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/index_text_color_selecter</item>
        <item name="android:textSize">@dimen/sp_16</item>
    </style>

    <style name="basic_public_rl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_50</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="basic_public_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/dp_15</item>
        <item name="android:textColor">@color/main_text_color</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <style name="public_rl_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:background">@color/gray_shallow</item>
    </style>

    <style name="public_rl_title_tv_desc">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="public_rl_title_app_image_button">
        <item name="android:layout_width">@dimen/dp_20</item>
        <item name="android:layout_height">@dimen/dp_20</item>
        <item name="android:layout_marginLeft">@dimen/dp_15</item>
        <item name="android:background">@drawable/icon_back_black</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="public_management_linear_module">
        <item name="android:layout_width">@dimen/dp_0</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">@dimen/dp_5</item>
        <item name="android:background">@drawable/public_bg_color_selecter</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="public_management_linear_module_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">@dimen/dp_8</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="public_management_linear_module_Image">
        <item name="android:layout_width">@dimen/dp_35</item>
        <item name="android:layout_height">@dimen/dp_35</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">@dimen/dp_20</item>
    </style>

    <style name="public_add_goods_rl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_60</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="public_add_goods_rl_title_image">
        <item name="android:layout_width">@dimen/dp_18</item>
        <item name="android:layout_height">@dimen/dp_18</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
    </style>
    <style name="public_arrow_image">
        <item name="android:layout_width">@dimen/dp_20</item>
        <item name="android:layout_height">@dimen/dp_20</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">@dimen/dp_10</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@drawable/icon_arrow</item>
    </style>

    <style name="public_add_goods_rl_title_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
        <item name="android:textColor">@color/main_text_color</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>
    <style name="public_add_goods_edit">
        <item name="android:layout_width">@dimen/dp_20</item>
        <item name="android:layout_height">@dimen/dp_20</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">@dimen/dp_20</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@drawable/icon_arrow</item>
    </style>
    <style name="public_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:layout_centerVertical">true</item>
    </style>


    <style name="public_add_goods_linear_et">
        <item name="android:layout_width">@dimen/dp_180</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
        <item name="android:layout_marginTop">@dimen/dp_5</item>
        <item name="android:layout_marginBottom">@dimen/dp_5</item>
        <item name="android:background">@null</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textCursorDrawable">@drawable/et_cursor</item>
        <item name="android:textSize">@dimen/sp_12</item>
        <item name="android:focusable">false</item>
    </style>
    <style name="public_add_goods_linear_image">
        <item name="android:layout_width">@dimen/dp_15</item>
        <item name="android:layout_height">@dimen/dp_15</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
        <item name="android:focusable">false</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="public_add_goods_linear_tv">
        <item name="android:layout_width">@dimen/dp_15</item>
        <item name="android:layout_height">@dimen/dp_15</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>
    <style name="public_add_goods_view_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_1</item>
        <item name="android:layout_marginLeft">@dimen/dp_5</item>
        <item name="android:layout_marginRight">@dimen/dp_5</item>
        <item name="android:background">@color/gray_shallow</item>
    </style>

    <style name="public_add_goods_linear_contain">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_50</item>
        <item name="android:orientation">horizontal</item>
    </style>


    <style name="carriage_template_rg_h_line">
        <item name="android:layout_width">@dimen/dp_0_5</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/gray_shallow</item>
    </style>
    <style name="carriage_template_rb_item" parent="@style/radio_group_item">
        <item name="android:paddingTop">@dimen/dp_10</item>
        <item name="android:textSize">@dimen/sp_12</item>
    </style>

    <style name="carriage_template_rg">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:layout_marginTop">@dimen/dp_5</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="rg_vertical">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginTop">@dimen/dp_5</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="rb_vertical">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_50</item>
        <item name="android:background">@color/white</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">@dimen/dp_3</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingLeft">@dimen/dp_30</item>
        <item name="android:textColor">@drawable/index_text_color_selecter</item>
        <item name="android:textSize">@dimen/sp_16</item>
    </style>

    <style name="carriage_template_rg_v_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_0_5</item>
        <item name="android:background">@color/gray_shallow</item>
    </style>
</resources>
